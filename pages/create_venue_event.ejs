    <script>
      // Image preview functionality for event banner
      document
        .getElementById("eventBanner")
        .addEventListener("change", function (e) {
          const file = e.target.files[0];
          if (file) {
            const reader = new FileReader();
            reader.onload = function (event) {
              document.getElementById("eventBannerImage").src =
                event.target.result;
            };
            reader.readAsDataURL(file);
          }
        });

      const saveSingleTicketBtn = document.getElementById("saveSingleTicket");
      const createEventBtn = document.getElementById("createEventBtn");

      saveSingleTicketBtn.addEventListener("click", () => {
        const ticketDetails = {
          ticketName: document.getElementById("ticketName").value,
          ticketOrder: document.getElementById("ticketOrder").value,
          ticketDescription: document.getElementById("ticketDescription").value,
          ticketPrice: document.getElementById("ticketPrice").value,
          variationName: document.getElementById("variationName").value,
        };
        console.log(ticketDetails);
      });

      createEventBtn.addEventListener("click", () => {
        const formData = new FormData();

        formData.append(
          "eventName",
          document.getElementById("eventName").value
        );
        formData.append("category", document.getElementById("category").value);
        formData.append(
          "eventDate",
          document.getElementById("eventDate").value
        );
        formData.append(
          "eventTime",
          document.getElementById("eventTime").value
        );
        formData.append(
          "eventDuration",
          document.getElementById("eventDuration").value
        );
        formData.append(
          "eventBanner",
          document.getElementById("eventBanner").files[0]
        );
        formData.append(
          "eventDescription",
          document.getElementById("eventDescription").value
        );
        formData.append("eventOrganizer", "DummyOrganizer");
        formData.append("venue", document.getElementById("venue").value);
        formData.append(
          "addressLine1",
          document.getElementById("addressLine1").value
        );
        formData.append(
          "addressLine2",
          document.getElementById("addressLine2").value
        );
        formData.append("country", document.getElementById("country").value);
        formData.append("state", document.getElementById("state").value);
        formData.append("city", document.getElementById("city").value);
        formData.append("zipCode", document.getElementById("zipCode").value);

        // tickets as JSON
        const tickets = [
          {
            ticketName: document.getElementById("ticketName").value,
            ticketOrder: document.getElementById("ticketOrder").value,
            ticketDescription:
              document.getElementById("ticketDescription").value,
            ticketPrice: document.getElementById("ticketPrice").value,
            variationName: document.getElementById("variationName").value,
          },
        ];

        formData.append("tickets", JSON.stringify(tickets));

        createEvent(formData);
      });

      // Function to create event
      function createEvent(formData) {
        // Show loader
        const loader = document.querySelector(".overlay-loader");
        loader.classList.add("active");

        fetch("/api/events", {
          method: "POST",
          body: formData, // NO JSON, NO HEADERS
        })
          .then((res) => res.json())
          .then((data) => {
            // Hide loader
            loader.classList.remove("active");

            if (data.success === true) {
              alert(data.message);
              // Optionally redirect to event page or clear form
              location.assign("/events/explore");
            } else {
              alert("Failed to create event");
            }
          })
          .catch((err) => {
            // Hide loader on error
            loader.classList.remove("active");

            console.error(err);
            alert("Failed to create event");
          });
      }
    </script>

